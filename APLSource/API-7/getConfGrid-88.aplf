 :If 'stats'≡NS.get'Sample'
         ⍝ Sample←1⌽')(',(-Parm≡'proportion')↓NS.get'Sample' 'SampSize' 'Mean' 'Sdev'
     Sample←1⌽')(',1 1(Parm≢'proportion')1/NS.get'Sample' 'SampSize' 'Mean' 'Sdev'
 :Else
     Sample←1⌽')(',NS.get'Sample'
 :EndIf
 :If 'stats'≡NS.get'Sample2'
